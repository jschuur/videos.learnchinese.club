{
  "name": "videos.learnchinese.club",
  "version": "0.0.2",
  "description": "Lists recent YouTube videos for Chinese language learning",
  "scripts": {
    "dev:client": "npm run dev --prefix client",
    "dev:server": "serverless offline --watch",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "npm run build --prefix client",
    "deploy": "serverless deploy --env production --stage prod",
    "deploy:client": "npm run build:client && serverless client deploy --no-confirm && aws cloudfront create-invalidation --distribution-id=E1UJA65L7ZLH79 --paths /",
    "update": "serverless invoke local -f update",
    "update:prod": "serverless invoke local -f update --env production",
    "update:both": "npm run update && npm run update:prod"
  },
  "keywords": [],
  "author": "Joost Schuur <jschuur@jschuur.com>",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.642.0",
    "ellipsize": "^0.1.0",
    "google-spreadsheet": "^3.0.10",
    "googleapis": "^48.0.0",
    "mongoose": "^5.9.5",
    "pluralize": "^8.0.0",
    "rss-parser": "^3.7.6",
    "saslprep": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "serverless": "^1.67.0",
    "serverless-bundle": "^1.3.2",
    "serverless-domain-manager": "^3.3.1",
    "serverless-dotenv-plugin": "^2.3.2",
    "serverless-finch": "^2.5.2",
    "serverless-offline": "^6.1.2"
  }
}
